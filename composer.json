{
    "name": "nikitanp/alfacrm-api-php",
    "description": "API for AlfaCRM",
    "keywords": [
        "nikitanp",
        "alfacrm-api-php",
        "alfacrm"
    ],
    "homepage": "https://github.com/nikitanp/alfacrm_api_php",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikita Mikhno",
            "email": "nikita.mikhnonp@gmail.com",
            "role": "Developer"
        }
    ],
    "suggest": {
        "guzzlehttp/guzzle": "great http client"
    },
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "guzzlehttp/guzzle": "^7.0",
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Nikitanp\\AlfacrmApiPhp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nikitanp\\AlfacrmApiPhp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
